#include "get_next_line.h"

char	*linefiller(char *dest, char *buf, char c)
{
	if (dest && buf && c)
		dest = ft_strrsub(buf, ft_strchr(buf, c),
						  ft_strrlen(ft_strchr(buf, c)));
}

